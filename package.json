{
  "name": "trial1",
  "version": "0.1.0",
  "private": true,
  "main": "public/Main.js",
  "homepage": "./addEntry",
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "antd": "^4.16.2",
    "concurrently": "^6.2.0",
    "electron": "^13.1.2",
    "electron-is-dev": "^2.0.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-to-print": "^2.0.0-alpha-1",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "npm run dev",
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "postinstall": "electron-builder install-app-deps",
    "pack-app": "npm run build-react && electron-builder --dir",
    "pack-build": "electron builder --dir",
    "build": "npm run build-react && electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-electron": "nodemon --watch ./public/* --exec \"electron .\"",
    "dev": "concurrently \"npm run start-react\" \"wait-on http://localhost:3000 && npm run start-electron\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.quebec.cylinder",
    "productName": "cylinder",
    "files": [
      "build/**/**"
    ],
    "directories": {
      "buildResources": "build"
    },
    "extraResourses": [
      {
        "from": "./db/",
        "to": "db/"
      }
    ],
    "win": {
      "target": [
        "7z"
      ]
    }
  }
}
