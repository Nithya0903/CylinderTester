{
  "name": "trial1",
  "version": "0.1.0",
  "private": true,
  "main": "public/Main.js",
  "homepage": "./ViewEntry",
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@react-navigation/native": "^6.0.8",
    "antd": "^4.18.3",
    "electron-builder": "^22.14.5",
    "moment": "^2.29.1",
    "node-gyp": "^8.4.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-to-print": "^2.14.3",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "npm run dev",
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "pack-app": "npm run build-react && electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "pack-build": "electron builder --dir",
    "build": "npm run build-react && electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-electron": "nodemon --watch ./public/* --exec \"electron --trace-warnings .\"",
    "dev": "concurrently \"npm run start-react\" \"wait-on http://localhost:3000 && npm run start-electron\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.quebec.cylinder",
    "productName": "cylinder",
    "files": [
      "build/**/**"
    ],
    "directories": {
      "buildResources": "build"
    },
    "extraResourses": [
      {
        "from": "./db/",
        "to": "db/"
      }
    ],
    "win": {
      "target": [
        "7z"
      ]
    }
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "electron": "^8.0.0",
    "electron-is-dev": "^2.0.0",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-scripts": "^5.0.0",
    "wait-on": "^6.0.0"
  }
}
